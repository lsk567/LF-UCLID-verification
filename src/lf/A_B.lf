target C;

reactor A {
	output _out:int;
	
	reaction(startup) -> _out {=
		set(_out, 1);
	=}
}

reactor B {
	input _in:int;
	state s:int;
	reaction(startup) {=
		self->s = 0;	
	=}
	reaction(_in) {=
		self->s = self->s + _in;
		printf("%d\n", self->s);
	=}
}

main reactor Composition {
	a = new A();
	b = new B();
	
	a._out -> b._in;
} 